<h1>Listing regions</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Name</th>
      <th>Qm min</th>
      <th>Qm max</th>
      <th>Multiplier min</th>
      <th>Multiplier max</th>
      <th>Multiplier avg</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @regions.each do |region| %>
      <tr>
        <td><%= region.user %></td>
        <td><%= region.name %></td>
        <td><%= region.qm_min %></td>
        <td><%= region.qm_max %></td>
        <td><%= region.multiplier_min %></td>
        <td><%= region.multiplier_max %></td>
        <td><%= region.multiplier_avg %></td>
        <td><%= link_to 'Show', region %></td>
        <td><%= link_to 'Edit', edit_region_path(region) %></td>
        <td><%= link_to 'Destroy', region, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Region', new_region_path %>
