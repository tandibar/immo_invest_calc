<%- model_class = Region -%>
<div class="page-header">
  <h1><%=t '.title', :default => Region %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:name) %></dt>
      <dd><pre class="prettyprint"><%= @region.name %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:qm_min) %></dt>
      <dd><pre class="prettyprint"><%= @region.qm_min %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:qm_max) %></dt>
      <dd><pre class="prettyprint"><%= @region.qm_max %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:multiplier_min) %></dt>
      <dd><pre class="prettyprint"><%= @region.multiplier_min %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:multiplier_max) %></dt>
      <dd><pre class="prettyprint"><%= @region.multiplier_max %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:multiplier_avg) %></dt>
      <dd><pre class="prettyprint"><%= @region.multiplier_avg %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              regions_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_region_path(@region), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              region_path(@region),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

